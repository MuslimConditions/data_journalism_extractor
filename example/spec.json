{
    "modules": [
        {
            "name": "extractor1",
            "type": "csvImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/data/deputes.csv",
            "dataType": ["String", "String"],
            "fieldDelimiter": ";",
            "namedFields": ["nom", "twitter"]
        },
        {
            "name": "mongo_loader",
            "type": "mongoImporter",
            "dbName": "testdb",
            "collection": "publications",
            "requiredFields": ["lienPageTwitter", "denomination"]
        },
        {
            "name": "split_twitter_hatvp",
            "type": "split",
            "source": "mongo_loader",
            "delimiter": "/",
            "field": 1,
            "reduce": -1
        },
        {
            "name": "split_twitter_hatvp2",
            "type": "split",
            "source": "split_twitter_hatvp",
            "delimiter": "\\?",
            "field": 1,
            "reduce": 1
        },
        {
            "name": "extractor_lex",
            "type": "csvImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/data/lex.csv",
            "dataType": ["String", "String"],
            "fieldDelimiter": ","
        },
        {
            "name": "split_lex",
            "type": "split",
            "source": "extractor_lex",
            "delimiter": "|",
            "field": 2
        },
        {
            "name": "extractordb",
            "type": "dbImporter",
            "dbUrl": "jdbc:postgresql://localhost/twitter",
            "fieldNames": ["rt_name","screen_name"],
            "dataType": ["String", "String"],
            "query": "select rt_name, screen_name from (select rt_name, uid from (select us.screen_name as rt_name, rt.sid from retweetedstatuses as rt join users as us on (rt.uid=us.uid)) as sub join checkedstatuses as ch on (sub.sid=ch.sid)) as subsub join users on (subsub.uid=users.uid);",
            "filterNull": true
        },
        {
            "name": "join_extractordb_splittwit",
            "type": "join",
            "source1": "extractordb",
            "source2": "split_twitter_hatvp2",
            "field1": 1,
            "field2": 0,
            "leftFields": [0]
        },
        {
            "name": "join_extractordb_exctractor1",
            "type": "join",
            "source1": "extractordb",
            "source2": "extractor1",
            "field1": 1,
            "field2": 1,
            "leftFields": [0]
        },
        {
            "name": "join_dbhatvp_extractor1",
            "type": "join",
            "source1": "join_extractordb_splittwit",
            "source2": "extractor1",
            "field1": 0,
            "field2": 1,
            "leftFields": [2],
            "rightFields": [0]
        },
        {
            "name": "join_db1_hatvp",
            "type": "join",
            "source1": "join_extractordb_exctractor1",
            "source2": "split_twitter_hatvp2",
            "field1": 0,
            "field2": 0,
            "leftFields": [1],
            "rightFields": [1]
        },
        {
            "name": "extractor4",
            "type": "csvImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/data/wiki.csv",
            "dataType": ["String", "String"],
            "quoteCharacter": "$",
            "fieldDelimiter": "|"
        },
        {
            "name": "tryExtractorWordSim",
            "type": "extractorWordSim",
            "source1": "split_lex",
            "source2": "split_lex",
            "leftField": 2,
            "rightField": 2
        },
        {
            "name": "linking1",
            "type": "extractorLink",
            "source1": "extractor4",
            "source2": "mongo_loader",
            "sourceExtract": 2,
            "targetExtract": 2
        },
        {
            "name": "projection1",
            "type": "projection",
            "source": "linking1",
            "fields": [1, 4]
        },
        {
            "name": "output1",
            "type": "csvOutput",
            "source": "projection1",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/output/output_mention_wiki.csv"
        },
        {
            "name": "output2",
            "type": "csvOutput",
            "source": "join_dbhatvp_extractor1",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/output/output_dep_retweet_hatvp.csv"
        },
        {
            "name": "output3",
            "type": "csvOutput",
            "source": "join_db1_hatvp",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/output/output_hatvp_retweet_dep.csv"
        },
        {
            "name": "glob_union1",
            "type": "union",
            "source1": "projection1",
            "source2": "join_dbhatvp_extractor1"
        },
        {
            "name": "glob_union2",
            "type": "union",
            "source1": "glob_union1",
            "source2": "join_db1_hatvp"
        },
        {
            "name": "globCount",
            "type": "countDistinct",
            "source": "glob_union2",
            "fields": [0, 1]
        },
        {
            "name": "globOutput",
            "type": "csvOutput",
            "source": "globCount",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/output/output_all.csv"
        }
    ]
}