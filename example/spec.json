{
    "modules": [
        {
            "name": "extractor1",
            "type": "csvImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/data/deputes.csv",
            "dataType": ["String", "String", "String"],
            "fieldDelimiter": ";",
            "namedFields": ["nom", "prenom", "twitter"]
        },
        {
            "name": "projection_twitter",
            "type": "projection",
            "source": "extractor1",
            "fields": [3]
        },
        {
            "name": "mongo_loader",
            "type": "mongoImporter",
            "dbName": "testdb",
            "collection": "publications",
            "requiredFields": ["lienPageTwitter", "denomination"]
        },
        {
            "name": "extractor_lex",
            "type": "csvImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/data/lex.csv",
            "dataType": ["String", "String"],
            "fieldDelimiter": ","
        },
        {
            "name": "split_lex",
            "type": "split",
            "source": "extractor_lex",
            "delimiter": "|",
            "field": 2
        },
        {
            "name": "extractordb",
            "type": "dbImporter",
            "dbUrl": "jdbc:postgresql://localhost/twitter",
            "fieldNames": ["rt_name","screen_name"],
            "dataType": ["String", "String"],
            "query": "select rt_name, screen_name from (select rt_name, uid from (select us.screen_name as rt_name, rt.sid from retweetedstatuses as rt join users as us on (rt.uid=us.uid)) as sub join checkedstatuses as ch on (sub.sid=ch.sid)) as subsub join users on (subsub.uid=users.uid);"
        },
        {
            "name": "extractor4",
            "type": "csvImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/data/wiki.csv",
            "dataType": ["String", "String"],
            "quoteCharacter": "$",
            "fieldDelimiter": "|"
        },
        {
            "name": "extractor2",
            "type": "jsonImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/data/hatvp.json",
            "mainField": "publications",
            "requiredFields": ["denomination", "identifiantNational", "activites", "lienPageTwitter"]
        },
        {
            "name": "split_twitter_hatvp",
            "type": "split",
            "source": "extractor2",
            "delimiter": "/",
            "field": 4
        },
        {
            "name": "tryExtractorWordSim",
            "type": "extractorWordSim",
            "source1": "split_lex",
            "source2": "split_lex",
            "leftField": 2,
            "rightField": 2
        },
        {
            "name": "linking1",
            "type": "extractorLink",
            "source1": "extractor4",
            "source2": "extractor2",
            "rightFields": [1],
            "sourceExtract": 1,
            "targetExtract": 2
        },
        {
            "name": "projection1",
            "type": "projection",
            "source": "linking1",
            "fields": [1, 3]
        },
        {
            "name": "output1",
            "type": "csvOutput",
            "source": "projection1",
            "path": "/Users/hugo/Work/limsi-inria/tests/data_journalism_extractor/example/output/output.csv"
        }
    ]
}