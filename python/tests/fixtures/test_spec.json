            "dataType": ["String", "String"],
            "fieldDelimiter": ";"
        },
        {
            "name": "extractor4",
            "type": "csvImporter",
            "path": "/Users/hugo/Work/limsi-inria/tests/test/wiki.csv",
            "dataType": ["String", "String"],
            "quoteCharacter": "$",
            "fieldDelimiter": "|"
        },
        {
